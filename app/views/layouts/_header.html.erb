
<header
class="header-section bgclr-teal text-white"
aria-labelledby="header"
>
<div class="container">
  <div class="row">
    <div class="col-12">
      <div class="d-flex align-items-center justify-content-md-between gap-3 gap-md-0" >
        <div class="order-2 order-md-0 d-flex align-items-center">
          <button
          class="btn p-0"
          type="button"
          data-bs-toggle="offcanvas"
          data-bs-target="#sidebar-navigation"
          aria-controls="sidebar-navigation" >
          <%= image_tag("nav-icon.svg", class: "img-fluid", width: "25.5px", height: "22.7px", alt: "mobile navigation icon") %>

        </button>
        <div class="p-0 ps-md-2 ps-lg-5 d-none d-md-flex align-items-center gap-2" >
          <%= image_tag("map-locator-icon.svg", class: "img-fluid", width: "24px", height: "24px", alt: "map map-locator-icon") %>
          <p class="m-0 p-0 fw-400 fs-pc-16 lh-pc-24 fs-tablet-9 lh-tablet-14 text-clr-white" >
            Hello Select Address
          </p>
        </div>
      </div>
      <div
      class="comapy-logo order-0 order-md-1 p-0 pe-md-5 me-auto me-md-0">
      <a href="/#">
        <%= image_tag("westar-header-logo-yellow.svg", class: "img-fluid", width: "153px", height: "40px", alt: "westar logo") %>
      </a>
    </div>
    <div class="d-md-none">
      <button class="p-0 btn">
        <%= image_tag("car-icons.svg", class: "img-fluid", width: "25.5px", height: "22.7px", alt: "mobile navigation icon") %>

      </button>
    </div>
    <div class="order-1 d-flex align-items-center">
      <a href="/#" class="d-none d-md-flex text-decoration-underline text-reset fs-tablet-9 lh-tablet-14 fs-pc-16 lh-pc-24 p-0 pe-md-2 pe-lg-5"> Track Order</a>
      <button class="btn p-0">
        <%= image_tag("header-cart-icon.svg", class: "img-fluid", width: "34px", height: "34px", alt: "cart icon") %>
      </button>
    </div>
  </div>
</div>
</div>
</div>
</header>
<div
class="offcanvas offcanvas-start"
tabindex="-1"
id="sidebar-navigation"
aria-labelledby="sidebar-navigation-label" >
<div class="offcanvas-header">
  <button
  type="button"
  class="btn-close"
  data-bs-dismiss="offcanvas"
  aria-label="Close"> </button>
</div>
<div class="offcanvas-body d-flex flex-column justify-content-between h-100">
  <div class="">
    <div class="d-flex justify-content-between align-items-center">
      <span>
        <%= image_tag 'sidebar-home-icon.svg', width: 24, height: 34, class: 'img-fluid', alt: 'sidebar home icon' %>
      </span>
      <a
      href="/#"
      class="text-reset text-decoration-none m-0 p-0 fw-400 fs-pc-16 lh-pc-24 text-clr-black"
      >
      Hello Select Address
      <span>
        <%= image_tag 'sidebar-map-locator-icon.svg', width: 24, height: 34, class: 'img-fluid ms-2', alt: 'sidebar map locator icon' %>
      </span>
    </a>
  </div>
  <div class="">
    <div class="d-flex gap-3 justify-content-start align-items-center py-3 border-bottom border-secondary-subtle">
      <span>
        <%= image_tag 'sidebar-chat-icon.svg', width: 24, height: 24, class: 'img-fluid', alt: 'sidebar chat icon' %>
      </span>
      <a
      href="/"
      class="text-reset text-decoration-none m-0 p-0 fw-400 fs-pc-16 lh-pc-24 text-clr-black"
      >Live Chat</a>
    </div>
    <div class="d-flex gap-3 justify-content-start align-items-center py-3 border-bottom border-secondary-subtle">
      <span>
        <%= image_tag 'sidebar-phone-icon.svg', width: 24, height: 24, class: 'img-fluid', alt: 'sidebar phone icon' %>
      </span>
      <a
      href="/"
      class="text-reset text-decoration-none m-0 p-0 fw-400 fs-pc-16 lh-pc-24 text-clr-black"
      >+1 888 374 5088</a>
    </div>
    <div class="d-flex gap-3 justify-content-start align-items-center py-3 border-bottom border-secondary-subtle">
      <span>
        <%= image_tag 'sidebar-file-icon.svg', width: 24, height: 24, class: 'img-fluid', alt: 'sidebar file icon' %>
      </span>
      <a
      href="/"
      class="text-reset text-decoration-none m-0 p-0 fw-400 fs-pc-16 lh-pc-24 text-clr-black"
      >Track Order</a>
    </div>
    <div class="d-flex gap-3 justify-content-start align-items-center py-3 border-bottom border-secondary-subtle">
      <span>
        <%= image_tag 'sidebar-file-icon.svg', width: 24, height: 24, class: 'img-fluid', alt: 'sidebar file icon' %>
      </span>
      <a
      href="/"
      class="text-reset text-decoration-none m-0 p-0 fw-400 fs-pc-16 lh-pc-24 text-clr-black"
      >My Account</a>
    </div>
  </div>
  <div class="py-4">
    <ul class="main-list m-0 p-0">
      <li class="list-item">
        <div class="d-flex justify-content-between align-items-center">
          <span class="fw-600 fs-pc-20 lh-pc-30 text-clr-black">Categories 1</span>
          <span class="icon">+</span>
        </div>
        <ul class="sublist">
          <li><span>Sublist 1-1</span></li>
          <li><span>Sublist 1-2</span></li>
          <li><span>Sublist 1-3</span></li>
        </ul>
      </li>
      <li class="list-item">
        <div class="d-flex justify-content-between align-items-center">
          <span class="fw-600 fs-pc-20 lh-pc-30 text-clr-black">Categories 2</span>
          <span class="icon">+</span>
        </div>
        <ul class="sublist">
          <li><span>Sublist 2-1</span></li>
          <li><span>Sublist 2-2</span></li>
          <li><span>Sublist 2-3</span></li>
        </ul>
      </li>
      <li class="list-item">
        <div class="d-flex justify-content-between align-items-center">
          <span class="fw-600 fs-pc-20 lh-pc-30 text-clr-black">Categories 3</span>
          <span class="icon">+</span>
        </div>
        <ul class="sublist">
          <li><span>Sublist 3-1</span></li>
          <li><span>Sublist 3-2</span></li>
          <li><span>Sublist 3-3</span></li>
        </ul>
      </li>
    </ul>
  </div>
</div>

<div class="mt-auto">
  <div class="d-flex justify-content-between align-items-center py-3">
    <div class="d-flex gap-3 justify-content-start align-items-center">
      <span class="flex-shrink-0">
        <%= image_tag 'circle-flags_us.svg', width: 24, height: 24, class: 'img-fluid rounded-circle flex-shrink-0', alt: 'US flag icon' %>
      </span>
      <a
      href="/"
      class="text-reset text-decoration-none m-0 p-0 fw-400 fs-pc-16 lh-pc-24 text-clr-black"
      >United States</a>
    </div>
    <div class="d-flex gap-3">
      <span class="text-uppercase fw-400 fs-pc-16 lh-pc-24 fw-500 text-clr-black">usd</span>
      <span>
        <%= image_tag 'sidebar-angle-icon.svg', width: 8, height: 8, class: 'img-fluid', alt: 'angle icon' %>
      </span>
    </div>
  </div>
</div>
<div class="d-flex py-3 align-items-center justify-content-around">
  <div class="d-flex flex-column align-items-center gap-3">
    <%= image_tag 'sidebar-phone-icon.svg', width: 24, height: 24, class: 'img-fluid', alt: 'phone icon' %>
    <a
    href="tel:+1 888 374 5088"
    class="text-decoration-none text-reset fw-400 fs-pc-16 lh-pc-24 text-clr-black"
    >About us</a>
  </div>
  <div class="d-flex flex-column align-items-center gap-3">
    <%= image_tag 'sidebar-phone-icon.svg', width: 24, height: 24, class: 'img-fluid', alt: 'phone icon' %>
    <a
    href="tel:+1 888 374 5088"
    class="text-decoration-none text-reset fw-400 fs-pc-16 lh-pc-24 text-clr-black"
    >Blog</a>
  </div>
  <div class="d-flex flex-column align-items-center gap-3">
    <%= image_tag 'sidebar-phone-icon.svg', width: 24, height: 24, class: 'img-fluid', alt: 'phone icon' %>
    <a
    href="tel:+1 888 374 5088"
    class="text-decoration-none text-reset fw-400 fs-pc-16 lh-pc-24 text-clr-black"
    >Help Centre</a>
  </div>
</div>
</div>
</div>
<div class="offcanvas-backdrop visually-hidden fade show"></div>