
<header
  class="header-section bgclr-teal text-white"
  aria-labelledby="header"
>
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div
          class="d-flex align-items-center justify-content-md-between gap-3 gap-md-0"
        >
          <div class="order-2 order-md-0 d-flex align-items-center">
            <button
              class="btn p-0"
              type="button"
              data-bs-toggle="offcanvas"
              data-bs-target="#sidebar-navigation"
              aria-controls="sidebar-navigation"
            >
                <%= image_tag("nav-icon.svg", class: "img-fluid", width: "25.5px", height: "22.7px", alt: "mobile navigation icon") %>

            </button>
            <div
              class="p-0 ps-md-2 ps-lg-5 d-none d-md-flex align-items-center gap-2"
            >
                <%= image_tag("map-locator-icon.svg", class: "img-fluid", width: "24px", height: "24px", alt: "map map-locator-icon") %>

              <p
                class="m-0 p-0 fw-400 fs-pc-16 lh-pc-24 fs-tablet-9 lh-tablet-14 text-clr-white"
              >
                Hello Select Address
              </p>
            </div>
          </div>
          <div
            class="comapy-logo order-0 order-md-1 p-0 pe-md-5 me-auto me-md-0"
          >
            <a href="/#">
            <%= image_tag("map-locator-icon.svg", class: "img-fluid", width: "153px", height: "40px", alt: "westar logo") %>
            </a>
          </div>
          <div class="d-md-none">
            <button class="p-0 btn">
            <%= image_tag("car-icons.svg", class: "img-fluid", width: "25.5px", height: "22.7px", alt: "mobile navigation icon") %>

            </button>
          </div>
          <div class="order-1 d-flex align-items-center">
            <a
              href="/#"
              class="d-none d-md-flex text-decoration-underline text-reset fs-tablet-9 lh-tablet-14 fs-pc-16 lh-pc-24 p-0 pe-md-2 pe-lg-5"
              >Track Order</a
            >
            <button class="btn p-0">
            <%= image_tag("header-cart-icon.svg", class: "img-fluid", width: "34px", height: "34px", alt: "cart icon") %>
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>
<div
